<div class="container">
	
	<table class="table table-hover">
		<thead>
			<th>
				
			</th>
		</thead>
		<tbody>
			<% @cards.each do |card|%>
			<tr>
				<td><%= card.card_value %></td>
				<td><%= card.user.name %></td>
				<td><%= link_to 'Show', card %></td>
				<td><%= link_to 'Edit', edit_user_card_path(card.user, card) %></td>
				<td><%= link_to 'Destroy', user_card_path(card.user, card), :confirm => 'Are you sure?', :method => :delete %></td>
			</tr>
			<%end%>
		</tbody>
		
		
	</table>
	
	
	
	
	
</div>
