

<div class="container">
	
	<%= render 'shared/supervisor_buttonbar'%>
	<div class="my_pager" id="pager">
	<%= render 'shared/pager', action: 'index', user: @user%>
	</div>
	<%if flash[:notice]%>
	<div class="alert alert-error">
  		<button type="button" id="close" class="close" data-dismiss="alert">&times;</button>
  		<strong>Noe gikk galt!</strong> <%=flash[:notice]%>
	</div>
	<%end%>
<div id="mainTable">
			<%= render 'shared/workdays', workdays: @workdays, user: @user%>
</div>
	
<%unless @workdays.empty?%>
	<%=button_to  "Godkjenn alle timer", {:action => "approve_all", id: @workday, user_id: @user},
					{class: "btn btn-small btn-success",
             		:confirm => "Are you sure?", method: :put}%>
             		<%end%>
</div>
